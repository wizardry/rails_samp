<h2>lgoin</h2>
<%= render "shared/flashMessage", _flash: flash.alert %>
<%= form_tag :session do %>
  <table class="loginForm" id="login_form">
    <tr>
      <th>user name</th>
      <td>
        <%= text_field_tag "name", "" %>
      </td>
    </tr>
    <tr>
      <th>pw</th>
      <td>
        <%= password_field_tag "password", "" %>
      </td>
    </tr>
    <tr>
      <td>
        <%= submit_tag "ログイン" %>
      </td>
    </tr>
  </table>
<% end %>